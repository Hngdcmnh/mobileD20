<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.trainingmobiled20.MainFragment"
        android:label="mainFragment"
        tools:layout="@layout/main_fragment">
        <action
            android:id="@+id/mainFragment_to_studentFragment"
            app:destination="@+id/studentFragment" />
    </fragment>

    <fragment
        android:id="@+id/studentFragment"
        android:name="com.example.trainingmobiled20.StudentFragment"
        android:label="studentFragment"
        tools:layout="@layout/student_fragment"/>


</navigation>